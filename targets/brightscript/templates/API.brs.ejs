function PlayFab<%- api.name %>Api() as object
    if m.PlayFab<%- api.name %>Api <> invalid
        return m.PlayFab<%- api.name %>Api
    end if

    m.PlayFab<%- api.name %>Api = {}

    <% for(var i in api.calls) { var apiCall = api.calls[i];
        %><%- getDeprecationAttribute("", apiCall)
        %>m.PlayFab<%- api.name %>Api.<%- apiCall.name %> = function(request = {}, extra_headers = invalid) as object
<%- generateApiSummary("        ", apiCall, "summary", "https://docs.microsoft.com/rest/api/playfab/" + api.name.toLowerCase() + "/" + apiCall.subgroup.toLowerCase().replaceAll(" ","-") + "/" + apiCall.name.toLowerCase())
%><%- getRequestActions("        ", apiCall) 
%>
        post = PlayFab().Post("<%- apiCall.url %>", request, <%- getAuthParams(apiCall) %>, extra_headers)<%- getResultActions("        ", apiCall, api) %>
        return post
    end function

    <% }
    %>return m.PlayFab<%- api.name %>Api
end function